{
    "manifest_version": 3,
    "name": "NSFW Filter v1.2",
    "version": "1.1",
    "description": "Filter NSFW words from websites.",
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_icon": "icon.png",
      "default_title": "NSFW Filter",
      "default_popup": "popup.html"
    }
  }
  